<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>transaction</servlet-name>
        <servlet-class>.transaction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>history</servlet-name>
        <servlet-class>history</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>views</servlet-name>
        <servlet-class>views</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>transection</servlet-name>
        <servlet-class>transection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>money</servlet-name>
        <servlet-class>money</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>transaction</servlet-name>
        <url-pattern>/transaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>history</servlet-name>
        <url-pattern>/history</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>views</servlet-name>
        <url-pattern>/views</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>transection</servlet-name>
        <url-pattern>/transection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>money</servlet-name>
        <url-pattern>/money</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
